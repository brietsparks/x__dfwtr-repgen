{% extends 'base.html.twig' %}

{% block title %}
{{ parent() }} - New Subdivision Report
{% endblock %}

{% block body %}

<div id="top" class="row">
    <div class="page-header">
        <h2>New Subdivision Report <span class="glyphicon glyphicon-file" aria-hidden="true"></span> </h2>
    </div>
</div>
    
    
    
<div class="row">
    <div class="row">
    <div class="col-md-12">
        {% for type, flashMessages in app.session.flashbag.all() %} 
        {% for flashMessage in flashMessages %}
        <div class="alert alert-{{ type }}">
            {{ flashMessage|raw }}
        </div>
         {% endfor %} 
         {% endfor %} 
    </div>
</div>


    {% embed 'subdivisionreport/form.html.twig' with {'form': form} %}
    {% endembed %}

    <hr/>

<div class="form-group">
    <a class="btn btn-default" href="{{ path('subdivisionreport') }}">
        <span class="glyphicon glyphicon-list" aria-hidden="true"></span>
        Back to list
    </a>
</div>

</div>


{% endblock %}

